import { strings, fmt, runtime }

def Main(start any) (stop any) {
        fmt.Println<string>, strings.Split, strings.Join, runtime.Panic
        list_to_stream ListToStream<string>
        ---
        :start -> [
                'neva' -> split:data,
                '' -> split:delim
        ]
        split -> list_to_stream:data
        list_to_stream -> join:data
        '-' -> join:sep
        join -> println:data
        println:res -> :stop
        println:err -> panic
}

