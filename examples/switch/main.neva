import {
    fmt
    runtime
    strings
}

def Main(start any) (stop any) {
    print fmt.Print
    scanln fmt.Scanln
    upper strings.ToUpper
    lower strings.ToLower
    println fmt.Println<any>
    panic1 runtime.Panic
    panic2 runtime.Panic
    ---
    :start -> 'Enter the name: ' -> print -> scanln -> switch {
        'Alice' -> upper
        'Bob' -> lower
        _ -> panic1
    }
    [upper, lower] -> println:data
    println:res -> :stop
    println:err -> panic2
}
