import {
    fmt
    errors
}

def Main(start any) (stop any) {
    h Handler
    ---
    :start -> { MaybeInt::None -> h -> :stop }
}

type MaybeInt union {
    Int int
    None
}

def Handler(data MaybeInt) (res MaybeInt) {
    must_println errors.Must<MaybeInt, MaybeInt>{fmt.Println}
    ---
    :data -> must_println -> :res
}
