import {
	fmt
	regexp
	runtime
}

def Main(start any) (stop any) {
	println fmt.Println
    submatch regexp.Submatch
    panic runtime.Panic
    ---
    :start -> [
        '-axxxbyc-' -> submatch:data,
        'a(x*)b(y|z)c' -> submatch:regexp
    ]
    submatch:res -> println:data
    println:res    -> :stop
    [submatch:err, println:err] -> panic
}
