    Nested struct {
        Level MyNonEmptyRec
    }
}

type MyShortEnum union { Monday }
type MyNormalEnum union { Monday, Tuesday, Wednesday, Thursday, Friday, Saturday }
pub type MyNormalNewlinedEnum union {
    Monday, Tuesday, Wednesday
    Thursday, Friday, Saturday
}

pub type MyArr string
type MyArrNested string
