type MyInt int

type MyNum union { Int Int, Float Float }
type Int union { Int int, Int2 int }
pub type Float union { Float float, Float2 float }

type MyEmptyRec struct {}
type MyNonEmptyRec struct {
    foo MyInt
    bar MyNum
    baz MyEmptyRec
}
